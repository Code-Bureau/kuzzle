{
  "compilerOptions": {
    "rootDir": "docker/scripts",
    "listEmittedFiles": true,
    "module": "commonjs",
    "target": "es2020",
    "moduleResolution": "node",
    "sourceMap": true,
    "baseUrl": ".",
    "paths": {
      "*": [
        "node_modules/*",
        "src/types/*"
      ]
    },
    "esModuleInterop": true
  }
}